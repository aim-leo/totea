{
  "name": "totea",
  "version": "1.9.0",
  "description": "Express CRUD for RESTful APIs",
  "author": "aimleo",
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "crud",
    "totea",
    "totea-cli"
  ],
  "engines": {
    "node": ">=10.x",
    "npm": ">= 6.x"
  },
  "browserslist": [
    "maintained node versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:aim-leo/totea.git"
  },
  "scripts": {
    "cm": "npx git-cz",
    "release": "standard-version && npm run push && npm run pub2npm",
    "release:d": "standard-version --dry-run",
    "release:m": "standard-version --release-as minor && npm run push && npm run pub2npm",
    "release:md": "standard-version --release-as minor --dry-run",
    "push": "git push --follow-tags origin master && git push --follow-tags github master",
    "pub2npm": "nrm use npm && npm publish",
    "test": "nodemon test/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "express": "~4.16.0",
    "express-async-errors": "^3.1.1",
    "fastest-validator": "^0.6.17",
    "fs-extra": "^9.0.1",
    "http-errors": "~1.6.2",
    "import-modules": "^2.0.0",
    "md5": "^2.2.1",
    "mongoose": "^5.3.15",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "resize-img": "^2.0.0"
  },
  "devDependencies": {
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "standard-version": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
